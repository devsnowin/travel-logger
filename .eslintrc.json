{
  "parserOptions": {
    "project": ["./tsconfig.json"]
  },
  "extends": [
    "next/core-web-vitals",
    "airbnb-base",
    "airbnb-typescript",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "no-case-declarations": 0
  }
}
